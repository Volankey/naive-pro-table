{
  "name": "naive-ui-protable",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.js": "eslint  --fix",
    "*.ts": [
      "prettier --write",
      "eslint  --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never"
    ]
  },
  "scripts": {
    "format": "prettier --write **/src/**/*.{ts,js,md,vue} ",
    "typecheck": "pnpm run typecheck",
    "dev:example": "pnpm run -C example dev",
    "dev:pkg": "pnpm run  -C packages dev",
    "build:pkg": "pnpm run -C packages build ",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volankey/naive-pro-table.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Volankey/naive-pro-table/issues"
  },
  "homepage": "https://github.com/Volankey/naive-pro-table#readme",
  "dependencies": {
    "@vue/test-utils": "^2.0.0"
  }
}
